{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "social network for developer",
  "main": "server.js",
  "scripts": {
    "client-install": "npm start --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"lsof -t -i tcp:5000 | xargs kill -9\" \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Huy Truong",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.29.2",
    "mongoose": "^5.3.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-moment": "^0.8.4",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.5"
  }
}
